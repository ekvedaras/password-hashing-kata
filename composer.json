{
  "require-dev": {
    "pestphp/pest": "^v1.23.0"
  },
  "require": {
    "php": "^8.2"
  },
  "autoload": {
    "Hacking\\": "src"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}